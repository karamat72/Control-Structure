#include <iostream>
using namespace std;

int main() {
    char ch;
    cout << "Enter a character: ";
    cin >> ch;

    if (ch >= 'A' && ch <= 'Z') {
        cout << "UPPER CASE";
    }
    else if (ch >= 'a' && ch <= 'z') {
        cout << "LOWER CASE LETTER";
    }
    else {
        cout << "INVALID VALUE";
    }

    return 0;
}
